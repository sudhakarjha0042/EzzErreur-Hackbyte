{
  "short_name": "EzzErreur",
  "version": "1.0",
  "manifest_version": 3, 
  "name": "Your Code Booking Marking Tool",
  "permissions": [ 
    "activeTab"
], 

  "action":{
    "default_popup": "index.html",
    "default_title": "Latest Covid Report",
    "default_icon":{
      "16": "HackbyteS.png",
      "128":"EzzErreurLogoM.png"
    }
},

"content_scripts": [
  {
    "matches": ["https://github.com/*pull*"],
    "js":  ["contentScripts/content.js"]
  }
]
}
